{% macro display_menu_item(item, loop) %}
	<tr>
		<td class="center">{{item.label}}</td>
		<td class="center">{% if item.latestMC is defined %}{{item.latestMC}}{% endif %}</td>
	</tr>
{% endmacro %}

{% from _self import display_menu_item %}

{% for item in menu %}
    {% if item.label is defined %}
        {{ display_menu_item(item, loop) }}
    {% endif %}
{% endfor %}
